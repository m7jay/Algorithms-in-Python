    "def benchmark(func):\n",
    "    import time\n",
    "    def wrapper(*args, **kwargs):\n",
    "        t = time.clock()\n",
    "        res = func(*args, **kwargs)\n",
    "        print(\"\\t\"+func.__name__, time.clock()-t)\n",
    "        return res\n",
    "    return wrapper"
   ]
  }
